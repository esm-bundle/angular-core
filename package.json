{
  "name": "@esm-bundle/angular__core",
  "version": "9.1.3-fix.0",
  "description": "Repo to test esm-bundle's automatic publishing",
  "type": "module",
  "exports": {
    ".": "./esm/es2015/angular-core.js"
  },
  "scripts": {
    "test": "yarn build && concurrently -n w: 'yarn:test:*'",
    "test:browser": "karma start karma.conf.cjs --single-run",
    "debug:test:browser": "karma start karma.conf.cjs",
    "test:formatting": "prettier --check .",
    "build": "rimraf system && webpack --config webpack.config.cjs",
    "format": "prettier --write .",
    "release": "release-it",
    "prepublishOnly": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn test"
    }
  },
  "files": [
    "esm",
    "system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esm-bundle/angular__core.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/esm-bundle/angular__core/issues"
  },
  "homepage": "https://github.com/esm-bundle/angular__core#readme",
  "devDependencies": {
    "@angular/compiler": "^11.1.1",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/core": "11.1.1",
    "@babel/core": "^7.9.0",
    "@babel/parser": "^7.9.4",
    "@babel/types": "^7.9.5",
    "@ngtools/webpack": "^11.1.2",
    "@types/babel__parser": "^7.1.1",
    "esm-bundle-scripts": "1.2.0",
    "karma": "^6.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "npm:@esm-bundle/rxjs@6.5.5-fix.1",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}
